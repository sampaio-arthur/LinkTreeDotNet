@model IEnumerable<LinktreeApp.Models.Link>

    <h1>Links</h1>

    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Url</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var link in Model)
        {
            <tr>
                <td>@link.Title</td>
                <td><a href="@link.Url" target="_blank">@link.Url</a></td>
            </tr>
        }
        </tbody>
    </table>

    <a href="/Links/Create">Add New Link</a>
